    "# panel (a) of return time cdf plot\n",
    "dats = sorted([d for d in data if d['l']==a and d['flow']!='l'],key=lambda x: x['flow'])\n",
    "for d in dats:\n",
    "    t,cdf = d['rt_cdf'].T\n",
    "    t = t/3600\n",
    "    plt.loglog(t,cdf,color=d['color'],label='flow ${}$, $l={}a$'.format(d['flow'],round(d['l']/a,1)))\n",
    "plt.xlim(1e-7,1e3)\n",
    "plt.ylim(1e-6,1e2)\n",
    "plt.legend()\n",
